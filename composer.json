{
  "name": "zrcms/core",
  "description": "ZRCMS - Content management for version tracking and content",
  "type": "library",
  "license": "BSD-3-Clause",
  "keywords": [
    "reliv",
    "content",
    "manager",
    "zend",
    "expressive",
    "middleware",
    "module"
  ],
  "homepage": "http://www.reliv.com/",
  "authors": [
    {
      "name": "James Jervis",
      "email": "jjervis@relivinc.com",
      "homepage": "http://www.reliv.com/"
    },
    {
      "name": "Rod McNew",
      "email": "rmcnew@relivinc.com",
      "homepage": "http://www.reliv.com/"
    }
  ],
  "require": {
    "php": ">=7",
    "psr/container": "^1",
    "psr/http-message": "^1",
    "rcm/api-lib": "^0.10",
    "reliv/zf-config-factories": "^4",
    "reliv/zf-input-filter-service": "^0.1",
    "zendframework/zend-expressive": "^1.* || 2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.*",
    "squizlabs/php_codesniffer": "3.*"
  },
  "autoload": {
    "psr-4": {
      "Zrcms\\": "/",
      "Zrcms\\Acl\\": "acl/",
      "Zrcms\\Cache\\": "cache/",
      "Zrcms\\Content\\": "content/",
      "Zrcms\\ContentCore\\": "content-core/",
      "Zrcms\\ContentCoreConfigDataSource\\": "content-core-config-data-source/",
      "Zrcms\\ContentCoreDoctrineDataSource\\": "content-core-doctrine-data-source/",
      "Zrcms\\ContentCountry\\": "content-country/",
      "Zrcms\\ContentDoctrine\\": "content-doctrine/",
      "Zrcms\\ContentLanguage\\": "content-language/",
      "Zrcms\\ContentRedirect\\": "content-redirect/",
      "Zrcms\\ContentRedirectDoctrineDataSource\\": "content-redirect-doctrine-data-source/",
      "Zrcms\\HttpExpressive1\\": "http-expressive-1/src/",
      "Zrcms\\HttpResponseHandler\\": "http-response-handler/",
      "Zrcms\\Importer\\": "importer/src/",
      "Zrcms\\Install\\": "install/src/",
      "Zrcms\\Locale\\": "locale/",
      "Zrcms\\Mustache\\": "mustache/",
      "Zrcms\\Param\\": "param/",
      "Zrcms\\ServiceAlias\\": "service-alias/",
      "Zrcms\\User\\": "user/",
      "Zrcms\\ViewAssets\\": "view-assets/",
      "Zrcms\\ViewHead\\": "view-head/src/",
      "Zrcms\\ViewHtmlTags\\": "view-html-tags/",
      "Zrcms\\XampleComponent\\": "xample-component/src/"
    }
  },
  "suggest": {
    "rwoverdijk/assetmanager": "A way of combining and delivering js and css from server",
    "zendframework/zend-diactoros": "Should install with Expressive",
    "rcm/user": "Decent user library"
  }
}
